networks:
  backend: {}
  frontend: {}
services:
  catalog:
    build:
      context: C:\Github\eCommerce\ProductCatalogAPI
      dockerfile: Dockerfile
    container_name: catalogapi
    depends_on:
    - mssqlserver
    environment:
      DatabaseName: CatalogDb
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      DatabaseUserPassword: ProductApi(!)
    image: products/catalogapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 5030
      target: 80
  mssqlserver:
    container_name: mssqlcontainer
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: ProductApi(!)
    image: microsoft/mssql-server-linux:latest
    networks:
      backend: null
    ports:
    - published: 1440
      target: 1433
version: '3.4'